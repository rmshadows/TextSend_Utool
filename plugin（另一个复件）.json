{
  "preload": "preload.js",
  "logo": "logo.png",
  "pluginName": "Textsend uTool插件",
  "author": "Ryan Yim",
  "version": "0.0.0",
  "platform": ["win32", "darwin", "linux"],
  "development": {
    "preload": "debug.js"
  },
  "features": [
    {
      "code": "debug",
      "explain": "debug测试",
      "platform": ["win32", "darwin", "linux"],
      "cmds": [
        "debug",
        "中文",
        {
          "type": "img",
          "label": "图片匹配"
        },
        {
          "type": "files",
          "label": "文件匹配",
          "fileType": "file",
          "match": "/xxx/",
          "minLength": 1,
          "maxLength": 1
        },
        {
          "type": "regex",
          "label": "文本正则匹配",
          "match": "/xxx/i",
          "minLength": 1,
          "maxLength": 1
        },
        {
          "type": "over",
          "label": "无匹配时",
          "exclude": "/xxx/i",
          "minLength": 1,
          "maxLength": 1
        },
        {
          "type": "window",
          "label": "窗口动作",
          "match": {
            "app": ["xxx.app", "xxx.exe"],
            "title": "/xxxx/",
            "class": ["xxx"]
          }
        }
      ]
    },
    {
      "code": "客户端",
      "explain": "TextSend Client",
      "platform": ["win32", "darwin", "linux"],
      "cmds": [
        "TextSend Client",
        "TextSend客户端"]
    }
  ]
}
